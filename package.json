{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest",
    "test:unit": "vitest --exclude '**/*.integration.test.ts'",
    "test:integration": "vitest --run --reporter=verbose **/*.integration.test.ts",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.58",
    "@iconify-json/simple-icons": "^1.2.44",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.0",
    "@nuxtjs/supabase": "^1.6.0",
    "nuxt": "^4.0.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.7.1",
    "@vite-pwa/nuxt": "^0.10.5",
    "@types/lodash-es": "^4.17.12",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.32.0",
    "happy-dom": "^15.11.7",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
